print("a = Conversion from inches to centimeters")
print("b = Conversion from centimeters to inches")
print("c = Guessing game")
print("d = What to watch?")
print("e = Sum of integers")
print("f = Quit")
print("")
g = input("Enter your choice: ")

while g != "f":
  
# Conversion from inches to centimeters

  if g == "a":
    l = int(input("Enter a number in inches: "))
    print("That is", l * 2.54, "centimeters")
    g = input("Enter your choice: ")
  
# Conversion from centimeters to inches

  elif g == "b":
    m = int(input("Enter a number in centimeters: "))
    print("That is", m / 2.54, "inches")
    g = input("Enter your choice: ")
    
# Guessing game

  elif g == "c":
    import random

    h = (random.randrange(1,5))

    guess = int(input("Guess a number between 1 and 5: ")) 
    while guess != h:
      if h < guess:
        print("Too High, Try again")
        guess = int(input("Guess a number between 1 and 5: "))
      elif h > guess:
        print("Too Low, Try again")
        guess = int(input("Guess a number between 1 and 5: "))
    else:
      print("You win")
      g = input("Enter your choice: ")
    
# What to watch?

  elif g == "d":
    i = print(input("Do you like sports? "))
    j = print(input("Do you like math? "))
    if i == "yes" and j == "yes":
      print("You should watch Moneyball")
    elif i == "yes" and j == "no":
      print("You should watch Dodgeball")
    elif i == "no" and j == "yes":
      print("You should watch Stand And Deliver")
    else:
      print("You should watch The Dark Knight")
      g = input("Enter your choice: ")
      
# Sum of integers

  elif g == "e":
    k = range(1, 347 + 1)
    print(sum(k))
    g = input("Enter your choice: ")
    
#quit
  
else:
  print("You have quit.")
